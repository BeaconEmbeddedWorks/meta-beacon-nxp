From 787156f0cd4c6c2f47649d5847fc38da5d224b33 Mon Sep 17 00:00:00 2001
From: Adam Ford <aford173@gmail.com>
Date: Wed, 20 Nov 2019 10:51:56 -0600
Subject: [PATCH 05/34] ARM64: dts: Change name of 'compatible' flag

Remove the reference to the fsl kit.
---
 arch/arm64/boot/dts/freescale/lpd-imx8mm-evk.dts | 4 ++--
 1 file changed, 2 insertions(+), 2 deletions(-)

diff --git a/arch/arm64/boot/dts/freescale/lpd-imx8mm-evk.dts b/arch/arm64/boot/dts/freescale/lpd-imx8mm-evk.dts
index f6dc5138f27b..557d934362f1 100644
--- a/arch/arm64/boot/dts/freescale/lpd-imx8mm-evk.dts
+++ b/arch/arm64/boot/dts/freescale/lpd-imx8mm-evk.dts
@@ -11,8 +11,8 @@
 #include "lpd-imx8mm-baseboard.dtsi"
 
 / {
-	model = "Logic PD i.MX8MM EVK board";
-	compatible = "fsl,imx8mm-evk", "fsl,imx8mm";
+	model = "Logic PD i.MX8M mini Developent Kit";
+	compatible = "fsl,imx8mm";
 
 	chosen {
 		bootargs = "console=ttymxc1,115200 earlycon=ec_imx6q,0x30890000,115200";
-- 
